{% set classname = admin.classnameLabel|lower %}
{% if classname == 'page' %}
    {% set route_name = 'app_' ~ classname %}
    
{% else %}
    {% set route_name = 'app_' ~ classname ~ "_show" %}
{% endif %}

{% set path_to_public = path(route_name, {slug: object.slug}) %}

<th>Titre</th>
<td>
    <a class="sonata-link-identifier" href="{{ path_to_public }}">
        {{ object.title }}
    </a>
</td>